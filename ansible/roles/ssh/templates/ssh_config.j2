Host *
  ForwardAgent              no
  ForwardX11                no
  ForwardX11Trusted         no
  CheckHostIP               no
  AddressFamily             inet
  IdentityFile              ~/.ssh/id_rsa
  Port                      22
  Protocol                  2
  Ciphers                   aes128-ctr,aes192-ctr,aes256-ctr,aes128-gcm@openssh.com,aes256-gcm@openssh.com,chacha20-poly1305@openssh.com
  KexAlgorithms             diffie-hellman-group-exchange-sha256,ecdh-sha2-nistp256,ecdh-sha2-nistp384,ecdh-sha2-nistp521,curve25519-sha256@libssh.org
  HostKeyAlgorithms         ssh-rsa,ssh-ed25519,ecdsa-sha2-nistp256,ecdsa-sha2-nistp384,ecdsa-sha2-nistp521,ssh-rsa-cert-v01@openssh.com,ssh-dss-cert-v01@openssh.com,ecdsa-sha2-nistp256-cert-v01@openssh.com,ecdsa-sha2-nistp384-cert-v01@openssh.com,ecdsa-sha2-nistp521-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
  SendEnv                   LANG LC_*
  HashKnownHosts            yes
  GSSAPIAuthentication      yes
  GSSAPIDelegateCredentials no
